#include <stdio.h>

void main() 
{
  int i,j,a,b;
  int arr[j];
  printf("Enter no of elemnts in the array:");
  scanf("%d",&j);
  for (i=0;i<j;i++)
  {
     scanf("%d",&arr[i]);
  }
  a=b=arr[0];
  for (i=0;i<j;i++)
  {
      if (arr[i]>a)
       a=arr[i];
  }
  for (i=0;i<j;i++)
  {
      if (arr[i]>b && arr[i]<a)
       b=arr[i];
  }
 printf("Second largest is %d",b);
}
